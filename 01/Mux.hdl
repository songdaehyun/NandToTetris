/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel , out=notsel );
    Or(a=sel , b=a , out=con1 );
    Or(a=notsel , b=b , out=con2 );
    And(a=con1 , b=con2 , out=out );

    //// Replace this comment with your code.
}